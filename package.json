{
  "name": "conquest-eth-website",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">= 12.17.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@tailwindcss/jit": "^0.1.18",
    "@tailwindcss/typography": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "autoprefixer": "^10.2.5",
    "cssnano": "^5.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "ipfs-deploy": "^8.0.1",
    "ipfs-gateway-emulator": "^4.2.1-ipfs.1",
    "mdsvex": "^0.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-load-config": "^3.0.1",
    "prepare-for-web": "^0.1.5",
    "prettier": "~2.3.0",
    "prettier-plugin-svelte": "^2.3.0",
    "prism-svelte": "^0.4.7",
    "remark": "^13.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-footnotes": "2.0",
    "remark-parse": "^9.0.0",
    "remark-slug": "^6.0.0",
    "remark-unwrap-images": "^2.0.0",
    "rimraf": "^3.0.2",
    "svelte": "^3.38.2",
    "svelte-preprocess": "^4.7.3",
    "sveltejs-adapter-ipfs": "^0.1.4",
    "tailwindcss": "^2.1.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "vite": "^2.3.3"
  },
  "scripts": {
    "prepare": "node ./.setup.cjs && prepare-for-web --target static --targetFile src/app.html",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "dev": "svelte-kit dev",
    "build": "rimraf docs && npm run prepare && svelte-kit build",
    "start": "svelte-kit start",
    "serve": "ipfs-emulator --only -d docs -p 8080",
    "deploy": "dotenv -- ipfs-deploy -p pinata -s conquest-eth-website docs",
    "build:deploy": "run-s build deploy",
    "build:serve": "run-s build serve"
  },
  "dependencies": {
    "named-logs": "^0.1.0"
  }
}
